<section id="{{ id is defined ? id : null }}">
    <h2>Les levels</h2>
    <div class="levels__container">
        {% for level in levels %}
            <a href="{{ path("labo_by_level", {level: level.name}) }}"
               style="background-color: {{ level.color }}">{{ level.name }}</a>
        {% endfor %}
        <a href="{{ path("labo_index") }}">Tous</a>
    </div>
</section>